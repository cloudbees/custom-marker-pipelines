pipeline {
agent any
  stages {
    stage('Test feature branch') {
      when {
        not { branch 'master'
          }
        }
      steps {
     container('maven') {
        sh """
        mvn test -Dcucumber.options="--tags @${env.BRANCH_NAME}"
        """
        }
      }
    }
    stage('Test master branch') {
      steps {
     container('maven') {
        sh """
        mvn test -Dcucumber.options="--tags @master"
        """
        }
      }
    }
  }
}
