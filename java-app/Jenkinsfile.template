pipeline {
  parameters {
    string(defaultValue: "", description: 'Cucumber Tag', name: 'CUCUMBER_TAG')
  }
  agent { label 'java-app' }
  stages {
    stage('Test') {

      steps {
     container(name:'maven',envVars:[envVar: key:'CUCUMBER_TAG',value:"${params.CUCUMBER_TAG}"]) {
       echo "$CUCUMBER_TAG"
        sh "mvn test"
        }
      }
    }
  }
}
